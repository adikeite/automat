
<label>
	{{input-file onUpload="import"}}
</label>

<h2>Imported records</h2>
<table>
  <tr>
	<th>Project</th>
	<th>Suite</th>
	<th>Name</th>
	<th>Created</th>
	<th></th>
  </tr>

	{{#each model.imported as |test|}}
			<tbody>
				<tr>
					<td>{{test.project}}</td>
					<td>{{test.suiteName}}</td>
					<td>{{test.name}}</td>
					<td>{{moment-from-now test.createdDate}}</td>
				</tr>
				<tr>
					<td colspan="2">{{markdown-to-html markdown=test.prerequisities}}</td>
					<td colspan="3">{{markdown-to-html markdown=test.steps}}</td>
				</tr>
			</tbody>
	{{/each}}
</table>
