<h1>Test Pool</h1>

<table>
  <tr>
    <th>Suite</th>
    <th>Name</th>
    <th>Created</th>
	<th></th>
  </tr>
  <tbody>
	{{#each model as |test|}}
		{{#unless test.isNew}}
			<tr {{action 'select' test}}>
		    	<td>{{test.suiteName}}</td>
		    	<td>{{test.name}}</td>
				<td>{{moment-from-now test.createdDate}}</td>
		    	<td><span {{action 'delete' test  bubbles=false}}>{{material-design-icon name='delete'}}</span></td>
		  	</tr>
	  	{{/unless}}
	{{/each}}
	</tbody>
</table>


{{outlet}}
